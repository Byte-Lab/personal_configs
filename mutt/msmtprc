defaults
	tls on
	tls_trust_file /etc/ssl/certs/ca-certificates.crt
	logfile ~/.msmtp.log

account manifault
	host smtp.gmail.com
	port 587
	auth xoauth2
	user void@manifault.com
	from void@manifault.com
	from_full_name  David Vernet
	tls_certcheck off
	tls_starttls on
	passwordeval /home/void/.personal_configs/bin/mutt_manifault.sh

account default : manifault
